{
  "/API/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {"^/API" : "http://localhost:8080/API"}
    }
  ,
  
    "/auth/*": {
    "target": "http://localhost:8080/",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
      "pathRewrite": {"^/auth" : "http://localhost:8080/"}
  
    }
}